<?xml version="1.0" ?>
<simple-type xmlns="http://www.sap.com/sap.ui.library.xsd" >

	<name>LabelDesign</name>

	<author>SAP AG</author>
	<documentation>Available label display modes.</documentation>
	<baseType>sap.ui.core/string</baseType>
	<enumeration>
		<value name="Bold">
		<documentation>Displays the label in bold.</documentation>
		</value>
		<value name="Standard">
		<documentation>Displays the label in normal mode.</documentation>
		</value>
	</enumeration>

</simple-type>
