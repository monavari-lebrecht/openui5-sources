<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<l:Grid
		defaultSpan="L12 M12 S12"
		width="auto">
		<l:content>
			<f:SimpleForm id="SimpleFormChange354"
				minWidth="1024"
				maxContainerCols="2"
				editable="true"
				layout="ResponsiveGridLayout"
				title="Address"
				labelSpanL="3"
				labelSpanM="3"
				emptySpanL="4"
				emptySpanM="4"
				columnsL="1"
				columnsM="1"
				class="editableForm">
				<f:content>
					<Label text="Name" />
					<Input value="{SupplierName}" />
					<Label text="Street/No." />
					<Input value="{Street}">
					</Input>
					<Input value="{HouseNumber}">
						<layoutData>
							<l:GridData span="L2 M2 S4" />
						</layoutData>
					</Input>
					<Label text="ZIP Code/City" />
					<Input value="{ZIPCode}">
						<layoutData>
							<l:GridData span="L2 M2 S4" />
						</layoutData>
					</Input>
					<Input value="{City}" />
					<Label text="Country" />
					<Select width="100%">
						<items>
							<core:Item text="Germany" />
							<core:Item text="USA" />
							<core:Item text="England" />
						</items>
					</Select>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>