<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<l:Grid
		defaultSpan="L12 M12 S12"
		width="auto">
		<l:content>
			<f:SimpleForm id="SimpleFormDisplay480"
				minWidth="1024"
				maxContainerCols="2"
				editable="false"
				layout="ResponsiveGridLayout"
				title="Address"
				labelSpanL="4"
				labelSpanM="4"
				emptySpanL="0"
				emptySpanM="0"
				columnsL="2"
				columnsM="2">
				<f:content>
					<core:Title text="Office" />
					<Label text="Name" />
					<Text text="{SupplierName}" />
					<Label text="Street/No." />
					<Text text="{Street} {HouseNumber}" />
					<Label text="ZIP Code/City" />
					<Text text="{ZIPCode} {City}" />
					<Label text="Country" />
					<Text text="{Country}" />
					<core:Title text="Online" />
					<Label text="Web" />
					<Text text="{Url}" />
					<Label text="Twitter" />
					<Text text="{Twitter}" />
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>