<mvc:View
	controllerName="sap.m.sample.InputSuggestionsOpenSearch.C"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<VBox
		class="marginBoxContent" >
		<items>
			<Label text="Product" />
			<Input
				id="productInput"
				type="Text"
				placeholder="Enter Product ..."
				showSuggestion="true"
				suggest="handleSuggest" />
		</items>
	</VBox>
</mvc:View>